<body class="img js-fullheight" style="background-image: url(images/bg-lg.jpg);">
	<section class="ftco-section" style="padding: 2em 0;">
		<div class="container">
			<div class="row justify-content-center">
				<div class="col-md-6 text-center mb-5">
					<h2 class="heading-section">Sign Up</h2>
				</div>
			</div>
			<div class="row justify-content-center">
				<div class="col-md-6 col-lg-4">
					<div class="login-wrap p-0">
						<form asp-page="/SignUp" class="signin-form">
							<div class="form-group">
								<input asp-for="SignUp.Email" class="form-control" placeholder="Email">
								<span asp-validation-for="SignUp.Email" class="text-danger ml-2"></span>
							</div>
							<div class="form-group">
								<input asp-for="SignUp.FirstName" class="form-control" placeholder="Firstname">
								<span asp-validation-for="SignUp.FirstName" class="text-danger ml-2"></span>
							</div>
							<div class="form-group">
								<input asp-for="SignUp.LastName" class="form-control" placeholder="Lastname">
								<span asp-validation-for="SignUp.LastName" class="text-danger ml-2"></span>
							</div>
							<div class="form-group">
								<input asp-for="SignUp.Password" type="password" class="form-control" placeholder="Password">
								<span toggle="#SignUp_Password" style="top: 35%;" class="fa fa-fw fa-eye field-icon toggle-password"></span>
								<span asp-validation-for="SignUp.Password" class="text-danger ml-2"></span>
							</div>
							<div class="form-group">
								<input asp-for="SignUp.ConfirmPassword" type="password" class="form-control" placeholder="Confirm Password">
								<span toggle="#SignUp_ConfirmPassword" style="top: 35%;" class="fa fa-fw fa-eye field-icon toggle-password"></span>
								<span asp-validation-for="SignUp.ConfirmPassword" class="text-danger ml-2"></span>
							</div>
							<div class="form-group">
								<button type="submit" class="form-control btn btn-primary submit px-3">Sign Up</button>
							</div>
							<div class="form-group d-md-flex justify-content-end">
								<div class="w-50 text-md-right">
									<a asp-page="/SignIn" style="color: #fff">Have an account?</a>
								</div>
							</div>
						</form>
					</div>
				</div>
			</div>
		</div>
	</section>
	<script src="~/js/popper.js"></script>
	<script src="~/lib/bootstrap/dist/js/bootstrap.min.js"></script>
	<script src="~/lib/jquery/dist/jquery.min.js"></script>
	<script defer src="https://static.cloudflareinsights.com/beacon.min.js/v52afc6f149f6479b8c77fa569edb01181681764108816" integrity="sha512-jGCTpDpBAYDGNYR5ztKt4BQPGef1P0giN6ZGVUi835kFF88FOmmn8jBQWNgrNd8g/Yu421NdgWhwQoaOPFflDw==" data-cf-beacon='{"rayId":"7de5f102b8fca123","token":"cd0b4b3a733644fc843ef0b185f98241","version":"2023.4.0","si":100}' crossorigin="anonymous"></script>
	<script src="~/js/main.js"></script>
	@{
		await Html.RenderPartialAsync("_ValidationScriptsPartial");
	}
</body>
</html>
